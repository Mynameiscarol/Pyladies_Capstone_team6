    "age_values = age_cat.value_counts(sort=False)\n",
    "steps = 10000\n",
    "maxy = round(max(age_values)/steps)*steps\n",
    "\n",
    "plt.figure()\n",
    "plt.bar(list(age_values.index), age_values.values)\n",
